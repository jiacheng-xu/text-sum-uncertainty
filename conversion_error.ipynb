   "source": [
    "import numpy as np\n",
    "import torch\n",
    "\n",
    "scores = torch.from_numpy(np.random.uniform(low=-100, high=100,size=100000))\n",
    "\n",
    "m = torch.nn.LogSoftmax()\n",
    "output = m(scores).numpy()\n",
    "output = np.asarray(output,dtype=np.float16).astype(np.float32)\n",
    "real_dist = np.exp(output)\n",
    "print(sum(real_dist))"
   ]
  }
